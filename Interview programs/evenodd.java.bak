import java.util.*;
class evenodd 
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		System.out.println("enter no:");
		int num = s.nextInt();
			if(num%2==0)
			{
			System.out.println("number is even");
			}
			else
				{
				System.out.println("num is odd");
			}
	}
}
